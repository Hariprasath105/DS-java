public class AgeCategory {
    public static int getAgeResult(int age) {
        if (age < 0 || age > 101) return -1;
        else if (age <= 5) return 0;
        else if (age <= 12) return 1;
        else if (age <= 19) return 2;
        else if (age <= 50) return 3;
        else if (age <= 60) return 4;
        else return 5;
    }

    public static void main(String[] args) {
        int age = 25;
        int ageResult = getAgeResult(age);
        System.out.println("Age Result: " + ageResult);
    }
}
